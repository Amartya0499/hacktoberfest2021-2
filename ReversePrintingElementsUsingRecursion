#include<iostream>
using namespace std;
void reversePrint(int a[],int n)
{
    if(n==0)
    {
        cout<<a[0]<<endl;
    }
    else
    {
      cout<<a[n-1]<<endl;
      reversePrint(a,n-1);
    }
}
int main()
{
    int arr[5];
    cout<<"Enter the array elements: "<<endl;
    for(int i=0;i<5;i++)
    {
        cin>>arr[i];
    }
    reversePrint(arr,5);
}
